{
  "name": "emoji-search",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"pnpm dev:astro\" \"pnpm dev:wrangler\"",
    "dev:astro": "astro dev",
    "dev:wrangler": "pnpm dlx wrangler dev --port 8787 --local --assets=\"\"",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "deploy": "astro build && pnpm dlx wrangler deploy"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.3",
    "@astrojs/react": "^4.3.0",
    "@electric-sql/pglite": "^0.3.7",
    "@huggingface/transformers": "^3.7.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "astro": "^5.13.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "wrangler": "^4.30.0"
  }
}